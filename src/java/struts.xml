<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <constant name="struts.devMode" value="true" />
    
    <package name="epay" namespace="/pages" extends="struts-default">
        <!-- login -->
        <action name="openLogin" class="epay.bisnis.openLogin">
            <result name="success">/pages/OpenLogin.jsp</result>
        </action>
        <action name="showLogin" class="epay.bisnis.showLogin">
            <result name="success">/pages/showLogin.jsp</result>
        </action>        
       <action name="dasboard" class="epay.bisnis.dasboard">
            <result name="success">/pages/dasboard.jsp</result>
        </action>   
         <action name="showMenu" class="epay.bisnis.showMenu">
            <result name="success">/pages/showMenu.jsp</result>
        </action>  
   
    
   </package>
</struts>